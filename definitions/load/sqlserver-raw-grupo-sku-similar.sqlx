INSERT INTO
  `RAW.cev_corp_grupo_sku_similar` ( 
	COD_GRUPO_SKU_SIMILAR,
	NOM_GRUPO_SKU_SIMILAR,
	COD_GRUPO_PRODUTO_SIMILAR,
	IND_ATIVO,
    PARTITIONTIME )
WITH
  TABELA AS (
  SELECT
	COD_GRUPO_SKU_SIMILAR,
	NOM_GRUPO_SKU_SIMILAR,
	COD_GRUPO_PRODUTO_SIMILAR,
	IND_ATIVO,
    _PARTITIONTIME AS PARTITIONTIME
  FROM
    `LAND.cev_corp_grupo_sku_similar` ),
  TABELA_B AS(
  SELECT
    MAX(PARTITIONTIME) AS PARTITIONTIME
  FROM
    `RAW.cev_corp_grupo_sku_similar` )
SELECT
  a.*
FROM
  TABELA a
WHERE
  a.PARTITIONTIME = (
  SELECT
    MAX(PARTITIONTIME)
  FROM
    TABELA)
